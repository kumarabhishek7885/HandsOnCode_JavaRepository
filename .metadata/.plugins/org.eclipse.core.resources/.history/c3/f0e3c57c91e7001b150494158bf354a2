package myPackage;

import java.util.Scanner;

public class Employee {

	public String name;
	public int age;
	
	public void readData() {
		Scanner obj = new Scanner(System.in);
		
		System.out.println("Enter name: ");	
		name = obj.nextLine();
		System.out.println("Enter age:");
		age = obj.nextInt();	
	}
	
	public void printData() {
		
		System.out.println();
		System.out.println();
		
	}
}

